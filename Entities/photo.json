{
  "name": "Photo",
  "type": "object",
  "properties": {
    "url": {
      "type": "string",
      "description": "Photo URL"
    },
    "caption": {
      "type": "string",
      "description": "Photo caption"
    },
    "uploaded_by": {
      "type": "string",
      "description": "Name of uploader"
    },
    "is_favorite": {
      "type": "boolean",
      "default": false
    },
    "latitude": {
      "type": "number",
      "description": "Photo geolocation latitude"
    },
    "longitude": {
      "type": "number",
      "description": "Photo geolocation longitude"
    },
    "ai_description": {
      "type": "string",
      "description": "AI-generated description of the photo"
    }
  },
  "required": [
    "url"
  ]
}
